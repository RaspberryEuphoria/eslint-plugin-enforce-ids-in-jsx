{
  "env": {
      "es6": true,
      "node": true,
      "mocha": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 9
  },
  "plugins": ["prettier", "import"],
  "rules": {
      "prettier/prettier": "error",
      "import/no-unresolved": [2, { "commonjs": true }],
      "import/named": 2,
      "import/namespace": 2,
      "import/default": 2,
      "import/export": 2,
      "strict": 0
  },
  "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
  ]
}
